{
  "name": "glass",
  "version": "0.0.0",
  "legacy-bundling": true,
  "lockfileVersion": 1,
  "scripts": {
    "ng": "ng",
    "postinstall": "patch-package; ngcc",
    "start": "ng serve --port 4242",
    "build": "ng build",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint:eslint": "ng lint",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint": "run-p -csl --aggregate-output lint:*",
    "fix:prettier": "prettier --write \"{src,cypress}/**/*.{ts,scss}\"",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix": "run-p -csl --aggregate-output fix:*",
    "e2e": "start-test 4242 'cypress open'",
    "e2e:ci": "start-test 4242 'cypress run -b chrome --headless'",
    "i18n": "npm run i18n:extract && npm run i18n:pull && npm run i18n:convert",
    "i18n:extract": "ngx-translate-extract --input src --output src/assets/i18n/aquarium.pot --sort --format pot --clean",
    "i18n:pull": "python3 -m venv venv; . venv/bin/activate; pip install transifex-client; tx --root=. pull --all --force",
    "i18n:convert": "run-p -csl --aggregate-output i18n:convert:*",
    "i18n:convert:de_DE": "po2json --format mf --pretty src/assets/i18n/de_DE.po src/assets/i18n/de_DE.json",
    "i18n:convert:zh_CN": "po2json --format mf --pretty src/assets/i18n/zh_CN.po src/assets/i18n/zh_CN.json",
    "i18n:convert:zh_TW": "po2json --format mf --pretty src/assets/i18n/zh_TW.po src/assets/i18n/zh_TW.json"
  },
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "^12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "^12.1.1",
    "@angular/compiler": "^12.1.1",
    "@angular/core": "^12.1.1",
    "@angular/flex-layout": "12.0.0-beta.34",
    "@angular/forms": "^12.1.1",
    "@angular/material": "^12.1.1",
    "@angular/platform-browser": "^12.1.1",
    "@angular/platform-browser-dynamic": "^12.1.1",
    "@angular/router": "^12.1.1",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@mdi/angular-material": "5.9.55",
    "@ngx-translate/core": "^13.0.0",
    "@swimlane/ngx-charts": "^17.0.1",
    "angular-gridster2": "^12.1.0",
    "dayjs": "^1.10.6",
    "lodash": "^4.17.21",
    "ng-block-ui": "3.0.2",
    "ngx-toastr": "^14.0.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.0",
    "validator": "^13.6.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.2.0",
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular-eslint/builder": "^12.3.0",
    "@angular-eslint/eslint-plugin": "^12.3.0",
    "@angular-eslint/eslint-plugin-template": "^12.3.0",
    "@angular-eslint/schematics": "^12.3.0",
    "@angular-eslint/template-parser": "^12.3.0",
    "@angular/cli": "^12.1.1",
    "@angular/compiler-cli": "^12.1.1",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@types/jasmine": "3.8.1",
    "@types/jest": "26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/node": "16.3.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "4.28.3",
    "@typescript-eslint/parser": "4.28.3",
    "codelyzer": "6.0.2",
    "cypress": "^7.7.0",
    "eslint": "^7.30.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.4.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-linter": "^1.1.1",
    "htmllint-cli": "0.0.7",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.1",
    "jest-preset-angular": "8.4.0",
    "npm-run-all": "4.1.5",
    "patch-package": "^6.4.7",
    "po2json": "^0.4.5",
    "prettier": "^2.3.2",
    "start-server-and-test": "^1.12.6",
    "ts-node": "10.1.0",
    "typescript": "~4.3.5"
  }
}
