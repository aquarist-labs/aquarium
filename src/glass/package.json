{
  "name": "glass",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "patch-package",
    "start": "ng serve --port 4242",
    "build": "ng build",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint:eslint": "ng lint",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint": "run-p -csl --aggregate-output lint:*",
    "fix:prettier": "prettier --write \"{src,cypress}/**/*.{ts,scss}\"",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix": "run-p -csl --aggregate-output fix:*",
    "e2e": "start-test 4242 'cypress open'",
    "e2e:ci": "start-test 4242 'cypress run -b chrome --headless'",
    "i18n:extract": "ngx-translate-extract --input src --output src/assets/i18n/aquarium.pot --sort --format pot --clean",
    "i18n:pull": "python3 -m venv venv; . venv/bin/activate; pip install transifex-client; tx --root=. pull --all --force",
    "i18n:convert": "run-p -csl --aggregate-output i18n:convert:*",
    "i18n:convert:de_DE": "po2json --format mf --pretty src/assets/i18n/de_DE.po src/assets/i18n/de_DE.json"
  },
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "~/(.*)$": "<rootDir>/src/$1"
    },
    "preset": "jest-preset-angular",
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "^11.2.11",
    "@angular/cdk": "^11.2.10",
    "@angular/common": "^11.2.11",
    "@angular/compiler": "^11.2.11",
    "@angular/core": "^11.2.11",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "^11.2.11",
    "@angular/material": "^11.2.10",
    "@angular/platform-browser": "^11.2.11",
    "@angular/platform-browser-dynamic": "^11.2.11",
    "@angular/router": "^11.2.11",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@mdi/angular-material": "5.9.55",
    "@ngx-translate/core": "^13.0.0",
    "@swimlane/ngx-charts": "^17.0.1",
    "angular-gridster2": "^11.1.5",
    "dayjs": "^1.10.4",
    "lodash": "^4.17.21",
    "ng-block-ui": "3.0.2",
    "ngx-toastr": "^13.2.1",
    "rxjs": "^6.6.7",
    "tslib": "^2.2.0",
    "validator": "^13.6.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.2.0",
    "@angular-devkit/build-angular": "0.1102.10",
    "@angular-eslint/builder": "^4.0.0",
    "@angular-eslint/eslint-plugin": "^4.0.0",
    "@angular-eslint/eslint-plugin-template": "^4.0.0",
    "@angular-eslint/schematics": "^4.0.0",
    "@angular-eslint/template-parser": "^4.0.0",
    "@angular/cli": "^11.2.10",
    "@angular/compiler-cli": "^11.2.11",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@types/jasmine": "3.6.9",
    "@types/jest": "26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "14.14.41",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "codelyzer": "6.0.1",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "32.3.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "html-linter": "^1.1.1",
    "htmllint-cli": "^0.0.7",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.1",
    "jest-preset-angular": "8.4.0",
    "npm-run-all": "4.1.5",
    "patch-package": "^6.4.7",
    "po2json": "^0.4.5",
    "prettier": "^2.2.1",
    "ts-node": "9.1.1",
    "typescript": "~4.1.5",
    "cypress": "^7.1.0",
    "start-server-and-test": "^1.12.1"
  }
}
